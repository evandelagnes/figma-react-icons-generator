    "url": "https://github.com/evandelagnes/figma-react-icons-generator/issues"
  },
  "homepage": "https://github.com/evandelagnes/figma-react-icons-generator#readme",
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "jest": "^29.2.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "figma-api-exporter": "^0.0.2"
  },
  "files": [
    "dist/**/*"
