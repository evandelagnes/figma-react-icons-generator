    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evandelagnes/figma-react-icons-generator.git"
  },
  "keywords": [
    "figma",
